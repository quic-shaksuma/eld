#---OverlayCommandInMap.test--------------------- Executable,LS------------------#
#BEGIN_COMMENT
# This test checks that OVERLAY blocks parsed from the linker script are printed
# into the text map file.
#END_COMMENT
#START_TEST
RUN: %clang %clangopts -c %p/Inputs/1.c -o %t.o
RUN: %link -MapStyle txt %linkopts -o %t.out %t.o -T %p/Inputs/script.t -Map %t.map
RUN: %filecheck %s < %t.map
#END_TEST

CHECK: # OVERLAY 0x{{[0-9A-Fa-f]+}} : NOCROSSREFS AT(0x{{[0-9A-Fa-f]+}})
CHECK: # {
CHECK: #{{.*}}.ov1
CHECK: #{{.*}}.ov2
CHECK: # } >ROM AT>RAM:PHDR1

