# Verify that extern symbol references are properly resolved in
# the linker script.
#END_COMMENT
#START_TEST
RUN: %clang %clangopts -o %t1.1.o -c %p/Inputs/1.c -ffunction-sections
RUN: %link %linkopts -o %t1.1.out %t1.1.o %p/Inputs/script.t
