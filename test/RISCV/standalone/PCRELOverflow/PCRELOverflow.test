UNSUPPORTED: riscv32
#----------PCRELOverflow.test--------------------- Executable------------------#

#BEGIN_COMMENT
#This test checks for relocation overflows from PCREL relocations
#to GP.
#--------------------------------------------------------------------
#END_COMMENT
#START_TEST
RUN: %clang %clangopts -fPIC -c %p/Inputs/t.c -ffunction-sections \
RUN: -fno-asynchronous-unwind-tables -o %t1.o -mno-relax
RUN: %not %link %linkopts  %t1.o -T %p/Inputs/script.t -o %t.out 2>&1 | %filecheck %s
#END_TEST

#CHECK: Error: {{.*}}1.o:(.text.main+0x1c): relocation R_RISCV_PCREL_HI20 out of range: -4160749596 is not in [-2147483648, 2147483647]; references '__var2'
#CHECK: Error: {{.*}}1.o:(.text.main+0x34): relocation R_RISCV_PCREL_HI20 out of range: 2415919052 is not in [-2147483648, 2147483647]; references '__var4'
